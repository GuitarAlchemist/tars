DESCRIBE {
    name: "TARS Autonomous Documentation Generation"
    version: "1.0"
    author: "TARS Autonomous System"
    purpose: "Use advanced reasoning to generate comprehensive PDF documentation and Jupyter notebooks"
    reasoning_driven: true
    output_formats: ["pdf", "jupyter_notebook", "markdown", "html"]
}

CONFIG {
    reasoning_capabilities_used: [
        "chain_of_thought_for_structure_planning",
        "dynamic_budgets_for_content_depth",
        "quality_metrics_for_documentation_excellence",
        "real_time_progress_for_generation_monitoring",
        "visualization_for_architecture_diagrams"
    ]
    
    documentation_targets: {
        comprehensive_pdf: {
            title: "TARS: The Autonomous Reasoning System - Complete Design Document"
            sections: [
                "executive_summary",
                "system_architecture", 
                "reasoning_capabilities",
                "implementation_details",
                "performance_analysis",
                "future_roadmap",
                "technical_appendices"
            ]
            target_pages: 50
            quality_threshold: 0.9
        }
        
        companion_notebook: {
            title: "TARS Interactive Design Exploration"
            sections: [
                "system_overview_interactive",
                "reasoning_demonstrations",
                "architecture_exploration",
                "performance_benchmarks",
                "code_examples",
                "visualization_gallery"
            ]
            target_cells: 100
            interactive_elements: true
        }
    }
    
    reasoning_approach: "comprehensive_analysis_with_structured_output"
}

AUTONOMOUS_DOCUMENTATION_STRATEGY {
    phase_1_reasoning_analysis: {
        description: "Use chain-of-thought reasoning to analyze TARS design comprehensively"
        reasoning_steps: [
            "observe_current_tars_architecture",
            "hypothesize_documentation_structure",
            "deduce_critical_design_elements",
            "analyze_causal_relationships_in_system",
            "synthesize_comprehensive_understanding",
            "validate_documentation_completeness"
        ]
        
        thinking_budget: {
            computational_units: 2000
            time_limit: "10_minutes"
            quality_threshold: 0.95
            strategy: "DeliberateAnalytical"
        }
    }
    
    phase_2_content_generation: {
        description: "Generate detailed content using reasoning-guided approach"
        content_types: [
            "architectural_descriptions",
            "technical_specifications",
            "design_rationale",
            "implementation_details",
            "performance_characteristics",
            "future_enhancements"
        ]
        
        quality_requirements: {
            accuracy: 0.95
            coherence: 0.90
            completeness: 0.85
            technical_depth: 0.90
        }
    }
    
    phase_3_visualization_integration: {
        description: "Create comprehensive diagrams and visualizations"
        visualization_types: [
            "system_architecture_diagrams",
            "reasoning_flow_charts",
            "component_interaction_graphs",
            "performance_metrics_charts",
            "timeline_visualizations"
        ]
        
        export_formats: ["svg", "png", "mermaid", "graphviz"]
    }
    
    phase_4_notebook_creation: {
        description: "Generate interactive Jupyter notebook with executable examples"
        notebook_features: [
            "interactive_code_cells",
            "live_demonstrations",
            "parameter_exploration",
            "visualization_widgets",
            "performance_benchmarks"
        ]
    }
}

DOCUMENTATION_CONTENT_STRUCTURE {
    pdf_document_outline: {
        section_1_executive_summary: {
            title: "Executive Summary"
            reasoning_focus: "strategic_overview_synthesis"
            content: [
                "tars_mission_and_vision",
                "key_capabilities_overview",
                "breakthrough_achievements",
                "business_value_proposition",
                "technical_innovation_summary"
            ]
            target_pages: 3
        }
        
        section_2_system_architecture: {
            title: "System Architecture"
            reasoning_focus: "architectural_analysis_and_design"
            content: [
                "high_level_architecture_overview",
                "core_component_descriptions",
                "reasoning_engine_architecture",
                "integration_patterns",
                "scalability_design",
                "security_architecture"
            ]
            target_pages: 12
            diagrams_required: 8
        }
        
        section_3_reasoning_capabilities: {
            title: "Advanced Reasoning Capabilities"
            reasoning_focus: "capability_deep_dive_analysis"
            content: [
                "chain_of_thought_reasoning_detailed",
                "dynamic_thinking_budgets_explanation",
                "quality_metrics_framework",
                "real_time_reasoning_architecture",
                "visualization_system_design",
                "integration_methodology"
            ]
            target_pages: 15
            code_examples: 20
        }
        
        section_4_implementation_details: {
            title: "Implementation Details"
            reasoning_focus: "technical_implementation_analysis"
            content: [
                "fsharp_implementation_rationale",
                "metascript_system_design",
                "qwen3_integration_approach",
                "performance_optimization_strategies",
                "testing_and_validation_framework",
                "deployment_architecture"
            ]
            target_pages: 10
            technical_depth: "high"
        }
        
        section_5_performance_analysis: {
            title: "Performance Analysis and Benchmarks"
            reasoning_focus: "performance_evaluation_synthesis"
            content: [
                "reasoning_performance_metrics",
                "scalability_benchmarks",
                "quality_assessment_results",
                "resource_utilization_analysis",
                "comparative_performance_studies",
                "optimization_recommendations"
            ]
            target_pages: 8
            charts_and_graphs: 15
        }
        
        section_6_future_roadmap: {
            title: "Future Roadmap and Enhancements"
            reasoning_focus: "strategic_planning_and_vision"
            content: [
                "short_term_enhancement_plans",
                "long_term_vision_and_goals",
                "research_and_development_priorities",
                "technology_evolution_roadmap",
                "community_and_ecosystem_development",
                "sustainability_and_maintenance_strategy"
            ]
            target_pages: 5
        }
        
        section_7_appendices: {
            title: "Technical Appendices"
            reasoning_focus: "comprehensive_reference_compilation"
            content: [
                "api_reference_documentation",
                "configuration_parameters_guide",
                "troubleshooting_and_diagnostics",
                "code_repository_structure",
                "dependency_specifications",
                "glossary_and_terminology"
            ]
            target_pages: 7
        }
    }
    
    jupyter_notebook_structure: {
        notebook_1_overview: {
            title: "TARS System Overview and Interactive Exploration"
            cells: [
                "introduction_and_setup",
                "system_architecture_visualization",
                "component_interaction_demo",
                "basic_reasoning_example",
                "performance_monitoring_dashboard"
            ]
            cell_count: 15
        }
        
        notebook_2_reasoning_demos: {
            title: "Advanced Reasoning Capabilities Demonstration"
            cells: [
                "chain_of_thought_interactive_demo",
                "dynamic_budget_exploration",
                "quality_metrics_analysis",
                "real_time_reasoning_simulation",
                "visualization_gallery"
            ]
            cell_count: 25
        }
        
        notebook_3_architecture_deep_dive: {
            title: "Architecture Deep Dive and Code Exploration"
            cells: [
                "core_component_analysis",
                "reasoning_engine_internals",
                "metascript_system_exploration",
                "integration_patterns_demo",
                "extensibility_examples"
            ]
            cell_count: 30
        }
        
        notebook_4_performance_benchmarks: {
            title: "Performance Benchmarks and Analysis"
            cells: [
                "reasoning_performance_tests",
                "scalability_analysis",
                "quality_assessment_benchmarks",
                "resource_utilization_monitoring",
                "optimization_experiments"
            ]
            cell_count: 20
        }
        
        notebook_5_future_exploration: {
            title: "Future Capabilities and Experimental Features"
            cells: [
                "experimental_reasoning_approaches",
                "prototype_implementations",
                "research_directions_exploration",
                "community_contribution_examples",
                "roadmap_interactive_planning"
            ]
            cell_count: 10
        }
    }
}

REASONING_DRIVEN_GENERATION_PROCESS {
    step_1_comprehensive_analysis: {
        reasoning_type: "meta_analytical"
        description: "Analyze entire TARS system to understand documentation requirements"
        inputs: [
            "all_tars_source_code",
            "metascript_specifications",
            "implementation_history",
            "performance_data",
            "user_feedback"
        ]
        
        reasoning_chain: [
            "observe_current_system_state",
            "hypothesize_documentation_needs",
            "deduce_critical_information_requirements",
            "analyze_audience_and_use_cases",
            "synthesize_optimal_documentation_structure",
            "validate_completeness_and_accuracy"
        ]
    }
    
    step_2_content_generation: {
        reasoning_type: "creative_analytical"
        description: "Generate high-quality technical content with reasoning validation"
        approach: [
            "section_by_section_reasoning",
            "content_quality_assessment",
            "technical_accuracy_validation",
            "coherence_and_flow_optimization",
            "visual_element_integration"
        ]
        
        quality_gates: [
            "technical_accuracy_check",
            "completeness_validation",
            "coherence_assessment",
            "readability_optimization"
        ]
    }
    
    step_3_visualization_creation: {
        reasoning_type: "visual_analytical"
        description: "Create comprehensive diagrams and visualizations"
        visualization_reasoning: [
            "identify_visual_communication_needs",
            "select_optimal_diagram_types",
            "design_clear_and_informative_visuals",
            "integrate_with_textual_content",
            "validate_visual_effectiveness"
        ]
    }
    
    step_4_notebook_generation: {
        reasoning_type: "interactive_educational"
        description: "Create engaging and educational interactive notebooks"
        notebook_reasoning: [
            "design_learning_progression",
            "create_interactive_demonstrations",
            "develop_hands_on_exercises",
            "integrate_real_time_examples",
            "validate_educational_effectiveness"
        ]
    }
}

OUTPUT_SPECIFICATIONS {
    pdf_document: {
        filename: "TARS_Complete_Design_Document.pdf"
        format: "professional_technical_document"
        styling: {
            font: "professional_technical"
            layout: "two_column_with_margins"
            color_scheme: "tars_brand_colors"
            header_footer: "comprehensive"
        }
        
        quality_requirements: {
            technical_accuracy: 0.95
            visual_appeal: 0.90
            readability: 0.85
            completeness: 0.90
        }
    }
    
    jupyter_notebooks: {
        format: "jupyter_notebook_v4"
        kernel: "fsharp_dotnet"
        dependencies: [
            "tars_engine_packages",
            "visualization_libraries",
            "data_analysis_tools",
            "interactive_widgets"
        ]
        
        interactive_features: [
            "live_code_execution",
            "parameter_exploration_widgets",
            "real_time_visualizations",
            "performance_monitoring_dashboards"
        ]
    }
    
    supporting_materials: {
        markdown_versions: "github_flavored_markdown"
        html_versions: "responsive_web_design"
        diagram_exports: ["svg", "png", "pdf"]
        code_examples: "syntax_highlighted"
    }
}

ACTION {
    type: "autonomous_documentation_generation"
    priority: "high"
    
    execution_steps: [
        "initialize_advanced_reasoning_system",
        "perform_comprehensive_tars_analysis",
        "generate_documentation_structure",
        "create_detailed_content_with_reasoning",
        "generate_visualizations_and_diagrams",
        "create_interactive_jupyter_notebooks",
        "validate_quality_and_completeness",
        "export_final_deliverables"
    ]
    
    success_criteria: [
        "comprehensive_pdf_document_generated",
        "interactive_jupyter_notebooks_created",
        "high_quality_visualizations_included",
        "technical_accuracy_validated",
        "user_experience_optimized"
    ]
    
    deliverables: [
        "TARS_Complete_Design_Document.pdf",
        "TARS_Interactive_Exploration.ipynb",
        "TARS_Reasoning_Demonstrations.ipynb", 
        "TARS_Architecture_Deep_Dive.ipynb",
        "TARS_Performance_Benchmarks.ipynb",
        "TARS_Future_Exploration.ipynb",
        "supporting_diagrams_and_visualizations"
    ]
}

