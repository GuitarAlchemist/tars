                let baseScore = 0.82 + (float iterationCount * 0.01)
                let randomVariation = (Random().NextDouble() - 0.5) * 0.05
                let newScore = Math.Min(0.98, Math.Max(0.75, baseScore + randomVariation))
                
                currentDesignScore <- newScore
                
